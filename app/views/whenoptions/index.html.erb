<p id="notice"><%= notice %></p>

<h1>Listing Whenoptions</h1>

<table>
  <thead>
    <tr>
      <th>Content</th>
      <th>Event</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @whenoptions.each do |whenoption| %>
      <tr>
        <td><%= whenoption.content %></td>
        <td><%= whenoption.event %></td>
        <td><%= link_to 'Show', whenoption %></td>
        <td><%= link_to 'Edit', edit_whenoption_path(whenoption) %></td>
        <td><%= link_to 'Destroy', whenoption, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Whenoption', new_whenoption_path %>
